			shoot(sac2,player)
		end
		
	
end



function sac3ai()
	
	
	if distance(sac3,player)>16 then
		--chase 'em down!
		if sac3.y>player.y then
			sac3.dy-=1
			elseif sac3.y<player.y then
				sac3.dy+=1
			end
		if sac3.x>player.x then
			sac3.dx-=1
			elseif sac3.x<player.x then
				sac3.dx+=1
		end
		
		if abs(sac3.dx)+
	abs(sac3.dy)>1.5 then
	
		if sac3.dx>0 then
			sac3.dx=0.5
			elseif sac3.dx<0 then
			sac3.dx=-0.5
		end
		if sac3.dy>0 then
			sac3.dy=0.5
			elseif sac3.dy<0 then
			sac3.dy=-0.5
		end
		sac3.x+=sac3.dx
		sac3.y+=sac3.dy
	end
	
	else
	if (slash.vis==false) and
	currenttime-sac3.counter>3 then
		slashattack()
		sac3.counter=currenttime
	end
	end
