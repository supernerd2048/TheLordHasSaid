function boxcheck(r1,r2)
	return r1.x < r2.x+r2.w and
         r1.x+r1.w > r2.x and
         r1.y < r2.y+r2.h and
         r1.y+r1.h > r2.y
end

function sac1ai()

--if close to wall,
	-- freeze and idk cry
	mapcollider={
	x=sac1.x-8,y=sac1.y-8,
	w=16,h=16
	}
	mapcollider2={
	x=sac1.x+8,y=sac1.y+8,
	w=16,h=16
	}
	if cmap(mapcollider) then
		sac1.scared=true
		sac1.y+=rnd(2)
		sac1.x+=rnd(2)
		sac1.y-=rnd(2)
		sac1.x-=rnd(2)
		return
	end
	if cmap(mapcollider2) then
		sac1.scared=true
		sac1.y+=rnd(2)
		sac1.x+=rnd(2)
		sac1.y-=rnd(2)
		sac1.x-=rnd(2)
		return
	end

	sac1.scared=false
if distance(sac1,player)<32 then
	if sac1.y<player.y then
		sac1.y-=1
		elseif sac1.y>player.y then
			sac1.y+=1
		end
	if sac1.x<player.x then
		sac1.x-=1
		elseif sac1.x>player.x then
			sac1.x+=1
		end
end
end


function sac2ai()
	if distance(sac2,player)<32 then
		--run for your life!
		if sac2.y<player.y then
			sac2.y-=2
			elseif sac2.y>player.y then
				sac2.y+=2
			end
		if sac2.x<player.x then
			sac2.x-=2
			elseif sac2.x>player.x then
				sac2.x+=2
		end
	end
	
	
		--everything is fine, shoot
		if (not bullet.vis) then
			shoot(sac2,player)
		end
		
	
end
