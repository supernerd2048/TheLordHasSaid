function cmap(thing)
  local ct=false
  local cb=false


  -- if colliding with map tiles

    local x1=thing.x/8
    local y1=thing.y/8
    local x2=(thing.x+7)/8
    local y2=(thing.y+7)/8
    local a=fget(mget(x1,y1),0)
    local b=fget(mget(x1,y2),0)
    local c=fget(mget(x2,y2),0)
    local d=fget(mget(x2,y1),0)
    ct=a or b or c or d

  return ct or cb
end

function checktrig1(thing)
  local ct=false
  local cb=false


  -- if colliding with map tiles

    local x1=thing.x/8
    local y1=thing.y/8
    local x2=(thing.x+7)/8
    local y2=(thing.y+7)/8
    local a=fget(mget(x1,y1),1)
    local b=fget(mget(x1,y2),1)
    local c=fget(mget(x2,y2),1)
    local d=fget(mget(x2,y1),1)
    ct=a or b or c or d

  return ct or cb
end

function readcheck(thing)
  local ct=false
  local cb=false


  -- if colliding with map tiles

    local x1=thing.x/8
    local y1=thing.y/8
    local x2=(thing.x+7)/8
    local y2=(thing.y+7)/8
    local a=fget(mget(x1,y1),2)
    local b=fget(mget(x1,y2),2)
    local c=fget(mget(x2,y2),2)
    local d=fget(mget(x2,y1),2)
    ct=a or b or c or d

  return ct or cb
end
