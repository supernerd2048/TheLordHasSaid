function _init() 
	player_init()
	currenttime=time()
	sac1={
	x=104,y=72,hp=6,
	w=8,h=8,dx=0,dy=0,
	hurt=0,scared=false
	}
	sac2={
	x=368,y=80,hp=6,
	w=8,h=8,hurt=0
	}
	bullet={
	x=0,y=0,vis=false,tim=0,
	w=7,h=7,dx=0,dy=2
	}
	sac3={
	x=300,y=300,hp=64
	}
	sac1trigger=true
	sac1killed=false
	sac2killed=false
	sac3killed=false
	cuts=true
end


function _update()
if not cuts then
	currenttime=time()
	bladetimecheck()
	shoottimecheck()
	bullettick()
	local lastplx=player.x
	local lastply=player.y
	local lastsac1x=sac1.x
	local lastsac1y=sac1.y
	local lastsac2x=sac2.x
	local lastsac2y=sac2.y
	player_input()
	sac1ai()
	--if not mget(43,23)==17 then
		sac2ai()
	--end
	collisionchecks()
	player.x+=player.dx
	player.y+=player.dy
	if cmap(player)==true then
		player.x=lastplx
		player.y=lastply
	end
	if cmap(sac1)==true then
		sac1.x=lastsac1x
		sac1.y=lastsac1y
	end
	if cmap(sac2)==true then
		sac2.x=lastsac2x
		sac2.y=lastsac2y
	end
	cls()
	pal(7,8,0)

	if currenttime-sac1.hurt>=0.1 then
		pal()
	end
	if not (currenttime-sac2.hurt>=0.1) then
		pal(1,8)
	end
	pal(9,133,1)
	camera(player.x-64,player.y-64)
	map()
	if player.facing==0 then
		spr(014, player.x, player.y)
		elseif player.facing==1 then
			spr(015,player.x,player.y,1,1,true)
		elseif player.facing==2 then
			spr(009,player.x,player.y)
		elseif player.facing==3 then
			spr(015,player.x,player.y)	
	end
	if blade.vis==true then
		spr(004,blade.x,blade.y)
	end
	if bullet.vis then
		spr(34,bullet.x,bullet.y)
	end

	spr(32,sac2.x,sac2.y)
	
	if sac1.scared then
		spr(021,sac1.x,sac1.y)
	 else
	 spr(005,sac1.x,sac1.y)
	end
	
else
	cutscene()
end
end

function player_init()
	player={}
	player.dx=0
	player.dy=0
	player.x=360
	player.y=288
	player.w=8
	player.h=8
	player.facing=0
	player.maxspeed=1.5
	blade={}
	blade.x=0
	blade.y=0
	blade.on=false
	blade.time=0
	blade.w=8
	blade.h=8
	blade.diry=0
	blade.dirx=0
end

function player_input()
	if (btn(‚û°Ô∏è)) then
		player.dx+=0.5;
		player.facing=1;
		player.dx=min(player.dx,1.5)
	end
	if (btn(‚¨ÖÔ∏è)) then
		player.dx-=0.5;
		player.dx=max(player.dx,-1.5)
		player.facing=3;
	end
	if not(btn(‚û°Ô∏è)or btn(‚¨ÖÔ∏è))then
		if (player.dx>0) then
			player.dx-=0.2
			elseif (player.dx<0) then
				player.dx+=0.2
		end
		if ((player.dx>-0.1)and(player.dx<0.05)) then
				player.dx=0
		end
	end	
	
	if (btn(‚¨áÔ∏è)) then
		player.dy+=0.5;
		player.facing=2;
		player.dy=min(player.dy,1.5)
	end
	if (btn(‚¨ÜÔ∏è)) then
		player.dy-=0.5;
		player.dy=max(player.dy,-1.5)
		player.facing=0;
	end
	if not(btn(‚¨áÔ∏è)or btn(‚¨ÜÔ∏è))then
		if (player.dy>0) then
			player.dy-=0.2
			elseif (player.dy<0) then
				player.dy+=0.2
		end
		if ((player.dy>-0.1)and(player.dy<0.05)) then
				player.dy=0
		end
	end
	
	--attack
	if (btnp(‚ùé)) then
		bladeattack()
	end
end


function collisionchecks()
	if blade.vis then
		if boxcheck(sac1,blade) and
		currenttime-sac1.hurt>0.1 then
			
			sac1.hp-=1
			sac1.x+=blade.dirx
			sac1.y+=blade.diry
			sac1.hurt=currenttime
			if sac1.hp<=0 then
				sac1.x=-99
				sac1.y=-99
				sac1killed=true
				cuts=true
				sfx(0)
				for x=1,5 do
					mset(42+x,23,26)
				end
			end
		end
			
		if boxcheck(sac2,blade) and
		currenttime-sac2.hurt>0.1 then
			sac2.hp-=1
			sac2.x+=blade.dirx
			sac2.y+=blade.diry
			sac2.hurt=currenttime
			if sac2.hp<=0 then
				sac2.x=-99
				sac2.y=-99
				sac2killed=true
				cuts=true
				sfx(0)
			end
		end
			
	
	end
end
