function bladetimecheck()
	if (currenttime-blade.time>=0.1) then
	blade.vis=false
	end
end



function shoot(shooter,victim)
	tim=currenttime
	bullet.vis=true
	bullet.x=shooter.x
	bullet.y=shooter.y
	bullet.dx=0
	bullet.dy=0
	
	--add tolerance for non-direct
	--vertical bullets
	if shooter.y>victim.y then
		bullet.dy=-2
	 elseif shooter.y<victim.y then
	 	bullet.dy=2
	end
	if shooter.x>victim.x then
		bullet.dx=-2
		elseif shooter.x<victim.x then
		bullet.dx=2
	end
end

--make var that stores
--positions at time of shooting
function bullettick()
	if bullet.vis then
		bullet.y+=bullet.dy
		bullet.x+=bullet.dx
	end
end

function shoottimecheck()
	if (currenttime-bullet.tim)>=2 then
	bullet.vis=false
	-- do not touch, idk why it
	-- works but it does‚¨áÔ∏è
	bullet.tim=currenttime
	end
end
